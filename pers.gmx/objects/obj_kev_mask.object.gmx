   {
   obj_kev_arms.sprite_index = spr_hold_weapon_arms_right // Arms
   obj_kev_palms.sprite_index = spr_hold_weapon_palms_right // Palms
   }
}

// Stand Animations
if keyboard_check(vk_nokey)
{
obj_kev_legs.sprite_index = spr_stand_legs // Legs
   
if obj_kev_head.sprite_index = spr_head_left
