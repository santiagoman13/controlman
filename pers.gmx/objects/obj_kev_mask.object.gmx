obj_shotgun.sprite_index = spr_shotgun_left // Shotgun

obj_rifle.sprite_index = spr_rifle_left // Rifle

   if obj_pistol.visible = true //If Hold Pistol
   {
   obj_kev_arms.sprite_index = spr_hold_pistol_arms_left // Arms
   obj_kev_palms.sprite_index = spr_hold_pistol_palms_left // Palms
   }
   
   if obj_shotgun.visible = true // If Hold Shotgun
   {
   obj_kev_arms.sprite_index = spr_hold_weapon_arms_left // Arms
   obj_kev_palms.sprite_index = spr_hold_weapon_palms_left // Palms
   }
   
   if obj_rifle.visible = true // If Hold Rifle
   {
   obj_kev_arms.sprite_index = spr_hold_weapon_arms_left // Arms
   obj_kev_palms.sprite_index = spr_hold_weapon_palms_left // Palms
   }
}

// Moving and Animations Right
if keyboard_check(ord("D")) - keyboard_check(ord("A"))
if place_free(x+4,y)
if kev_cover = false
{
x+=4;
obj_kev_legs.sprite_index = spr_move_legs // Legs
obj_kev_legs.image_speed = 0.3

obj_boots1.sprite_index = spr_boots1_move_right // Boots
obj_boots1.image_speed = 0.3

obj_kev_head.sprite_index = spr_head_right // Head

obj_pistol.sprite_index = spr_pistol_right // Pistol

